#include <stdio.h>
#include <string.h>

int main() {
    char name[100];
    fgets(name, sizeof(name), stdin);
    name[strcspn(name,"\n")]=0;

    printf("Initials: ");
    printf("%c", name[0]);
    for(int i=0;i<strlen(name);i++){
        if(name[i]==' ' && name[i+1]!='\0')
            printf("%c", name[i+1]);
    }
    printf("\n");

    return 0;
}

/*
Input:
John Michael Doe

Output:
Initials: JMD
*/
